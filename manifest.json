{
  "name": "Github Review",
  "description": "Construct your PR review notifications from a single click.",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html",
    "default_title": "Generate PR message"
  },
  "permissions": [
    "activeTab"
  ]
}
